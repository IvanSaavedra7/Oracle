package bd.daos;
import bd.core.*;

public class UsuariosTestes {

	public static MeuResultSet getSenhaUsuario() throws Exception
	{
		MeuResultSet resultado = null;
		try {
			String sql;
			sql = "select * from Alunos order by RA";
			BDSQLServer.COMANDO.prepareStatement (sql);	
		    resultado = (MeuResultSet)BDSQLServer.COMANDO.executeQuery ();  
		}catch (SQLException erro)
        {
            throw new Exception ("Falha ao recuperar alunos.");
        }
		return resultado;
	}
	
 	
}
